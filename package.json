{
  "name": "dismiss-alerts",
  "version": "2.0.2",
  "private": true,
  "description": "Dismiss GitHub Code Scanning alerts from SARIF suppression data",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/advanced-security/dismiss-alerts.git"
  },
  "keywords": [
    "sarif",
    "code-scanning",
    "ghas"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^7.0.0",
    "@octokit/plugin-retry": "^6.0.1",
    "console-log-level": "^1.4.1",
    "nock": "^13.5.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/console-log-level": "^1.4.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.2",
    "eslint-plugin-github": "^5.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
